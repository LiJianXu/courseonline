/* component/car/car.wxss */
.list{
  margin: 1px 0px;
  width: 100%;
  height: 100px;
  background-color: #fff;
}
.img{
  position: relative;
  float: left;
  margin: 10px auto 10px 10px;
  width: 38%;
  height: 80px;
}
.right{
  position: relative;
  float: right;
  margin: 10px 10px 10px auto;
  width: 54%;
  height: 80px;
}
.cartitle{
  position: relative;
  float: left;
  width: 95%;
  height: 66px;
  margin: 0px 5px;
}
.pricecontainer{
  position: relative;
  float: left;
  width: 95%;
  margin: 0px 5px;
  height: 14px;
}
.price{
  position: relative;
  float: left;
  color: #ff6600;
  font-size: 10px;
}
.conttitle{
    position: relative;
  float: right;
  font-size: 10px;
}
.cont{
      position: relative;
  float: right;
  font-size: 10px;
}
