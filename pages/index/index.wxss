/**index.wxss**/
.showpane{
  display: block;
}
.nonepane{
  display: none
}
button{
  padding:0px;
}
.banner{
  position:absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
}
.free{
  position: relative;
  float: left;
  width: 100%;
  height: 40px;
  margin: 5px 0px;
  background:#EBEBEB;
}
.freeleft{
  position: relative;
  float: left;
  background: #ff6600;
  width: 5px;
  height: 40px;
}
.freetext{
  position: relative;
  float: left;
  color: #000;
  font-size: 16px;
  width: 80px;
  height: 40px;
  margin-left: 5px;
  line-height: 40px;
}
.freeright{
  position: relative;
  float: right;
  height: 40px;
  font-size: 15px;
  color: rgba(175, 162, 162, 0.925);
  line-height: 40px;
  margin-right: 10px;
}
.entoicon{
  color:#80848f;
  position: relative;
  top: -2px;
}
.line{
  position: relative;
  float: left;
  width: 100%;
  height: 50px;
}
@import "/pages/course/course.wxss"